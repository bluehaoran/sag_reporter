<% provide(:title, 'All users') %>
<% provide(:context_title, 'Users') %>

<%= will_paginate %>

<ul class="users mdl-list">
  <%= render @users %>
</ul>

<%= will_paginate %>

<% if logged_in_user.admin? %>
  <%= link_to new_user_path, id: 'add-user-button', class: "mdl-button mdl-js-button mdl-button--fab mdl-js-ripple-effect mdl-button--colored" do %>
    <i class="material-icons">add</i>
  <% end %>
<% end %>
