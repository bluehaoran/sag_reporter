<% min_id = @language_stream.ministry_id %>
<% lang_id = @language_stream.state_language_id %>
<% facilitators = @language_stream.project.language_streams.where(ministry_id: min_id, state_language_id: lang_id) %>
var facList = $('td[data-ministry=<%= min_id %>][data-state-language=<%= lang_id %>]').find('.facilitator-list')
facList.html(
    '<%= j render partial: 'project_facilitator', collection: facilitators, locals: { stream_id: min_id, state_language_id: lang_id } %>'
);
facList.find('.set-sub-project').on('change', function () {
    $(this).closest('form').submit();
})
$('#add-facilitator-input-<%= lang_id %>-<%= min_id %>').val('');
$('#add-facilitator-choices-<%= lang_id %>-<%= min_id %>').empty();