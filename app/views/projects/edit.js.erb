$('#project-content').html('<%= j render(partial: 'form') %>');
componentHandler.upgradeElements($('#project-content').find('.mdl-tabs').get());
$('#project>-content .select-language').on('input', function(){
    const entry = this.value.toLowerCase();
    $('#project-content .add-language-possibilities li').hide();
    if (entry.length >= 3){
        console.log(entry);
        $('#project-content .add-language-possibilities li[data-lang-name*=\''+ entry +'\']').show();
    }
});
