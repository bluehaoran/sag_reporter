<% provide(:title, 'Report') %>
<%= render partial: "report", object: @report, locals: {time: false, languages: true, topics: true, wrapper: false} %>
<% if logged_in_user.can_create_report? %>
  <%= link_to new_report_path(@report), class: "waves-effect right btn" do %>
    <i class="material-icons left">create</i>
    Write another report
  <% end %>
<% end %>
